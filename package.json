  "dependencies": {
    "swagger-nodegen-cli": "git+https://git@github.com/johndcarmichael/swagger-nodegen-cli.git"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
